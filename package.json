{
  "name": "world-cup-of-tube-lines-api",
  "version": "0.1.0",
  "description": "A simple express API for the World Cup Of Tube Lines, run by Geoff Marshall",
  "main": "index.js",
  "repository": "https://github.com/davwheat/world-cup-of-tube-lines-api.git",
  "engines": {
    "node": ">=12.x"
  },
  "contributors": [
    {
      "name": "David Wheatley",
      "email": "hi@davwheat.dev",
      "url": "https://davwheat.dev"
    }
  ],
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "watch": "pm2 start index.js --watch --name \"WCOTL API\" -- --color",
    "restart": "yarn stop && yarn watch",
    "format": "prettier --write \"**/*.{js,json}\"",
    "stop": "pm2 stop index.js --watch",
    "status": "pm2 status",
    "monitor": "pm2 monit",
    "logs": "pm2 logs \"WCOTL API\""
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "shrink-ray-current": "^4.1.2"
  },
  "devDependencies": {
    "pm2": "^4.5.0",
    "prettier": "^2.1.2"
  },
  "optionalDependencies": {
    "node-zopfli-es": "~1.0.7"
  }
}
